{
	"explorer.fileNesting.patterns": {
		"*.ks": "${capture}.error, .${capture}.ks.*",
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/*.ksb": false,
		"**/*.ksh": false,
		"**/*.ksm": false,
		"**/*.marks": true,
		"coverage": true,
	},
	"search.exclude": {
		"node_modules": true,
		"**/*.ksb": true,
		"**/*.ksh": true,
		"**/*.ksm": true,
		"**/*.marks": true,
		"lib/compiler*.js": true,
		"coverage": true,
	},
}
